<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.canvas.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="500.0" prefWidth="600.0" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javafxproject.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="welcomeText" />
    <Canvas fx:id="canvas" height="600.0" width="600" />

    <Button onAction="#zaporedno" prefHeight="25.0" prefWidth="79.0" text="Zaporedno" />
    <Button fx:id="vzporedno" onAction="#vzporedno" prefHeight="25.0" prefWidth="83.0" text="Vzporedno" />
    <Button fx:id="porazdeljeno" onAction="#porazdeljeno" prefHeight="25.0" prefWidth="49.0" text="Clear" />


</VBox>